<h1>sigin up</h1>
<% if @user.errors.any? %>
  <ul>
    <% @user.errors.full_messages.each do |message| %>
      <li><%= message %></li>
    <% end %>
  </ul>
<% end %>
<div>
  <%= form_for(@user, url: sign_up_path, method: :post) do |f| %>
    <%= f.label :nickname %>
    <%= f.text_field :nickname %>
    <%= f.label :email %>
    <%= f.email_field :email %>
    <%= f.label :password %>
    <%= f.password_field :password %>
    <%= f.submit "sign up" %>
  <% end %>
</div>